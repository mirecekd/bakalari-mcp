# Bakaláři MCP Server Configuration
# Zkopírujte tento soubor jako .env a vyplňte své údaje

# Bakaláři přihlašovací údaje
BAKALARI_USER=tvuj_username
BAKALARI_PASSWORD=tvuj_password
BAKALARI_URL=https://your_school.bakalari.cz

# Python konfigurace
PYTHONPATH=/app
PYTHONUNBUFFERED=1

# Logging (volitelné)
FASTMCP_LOG_LEVEL=ERROR
